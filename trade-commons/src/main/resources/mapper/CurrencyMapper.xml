<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ybb.trade.mapper.CurrencyMapper">

    <resultMap id="BaseResultMap" type="com.ybb.trade.entity.Currency">
            <id property="name" column="name" jdbcType="VARCHAR"/>
            <result property="cnName" column="cn_name" jdbcType="VARCHAR"/>
            <result property="status" column="status" jdbcType="INTEGER"/>
            <result property="sort" column="sort" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        name,cn_name,status,
        sort
    </sql>
</mapper>
